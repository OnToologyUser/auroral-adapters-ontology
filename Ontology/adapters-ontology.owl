#################################################################
#    Object Properties
#################################################################

###  https://saref.etsi.org/core#controlsProperty
saref:controlsProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain saref:Device ;
                       rdfs:range saref:Property .


###  https://saref.etsi.org/core#measuresProperty
saref:measuresProperty rdf:type owl:ObjectProperty ;
                       rdfs:domain saref:Device ;
                       rdfs:range saref:Property ;
                       rdfs:comment "measures property" .


#################################################################
#    Data properties
#################################################################

###  https://auroral.iot.linkeddata.es/def/adapters#from-timestamp
adapters:from-timestamp rdf:type owl:DatatypeProperty ;
                        rdfs:domain adapters:MonitoringSensor ,
                                    adapters:WeatherSensor ;
                        rdfs:comment "UTC time in ISO 8601 formatted timestamp for when to get data From" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasHeight
adapters:hasHeight rdf:type owl:DatatypeProperty ;
                   rdfs:domain adapters:SoilMoisture ;
                   rdfs:range xsd:float ;
                   rdfs:label "has height" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasVolume
adapters:hasVolume rdf:type owl:DatatypeProperty ;
                   rdfs:domain adapters:SoilMoisture ;
                   rdfs:range xsd:float ;
                   rdfs:label "has volume" .


###  https://auroral.iot.linkeddata.es/def/adapters#to-timestamp
adapters:to-timestamp rdf:type owl:DatatypeProperty ;
                      rdfs:comment "UTC time in ISO 8601 formatted timestamp for when to get data TO" .


###  https://auroral.iot.linkeddata.es/def/core#hasId
core:hasId rdf:type owl:DatatypeProperty ;
           rdfs:domain saref:Device ;
           rdfs:range xsd:string ;
           rdfs:label "has id" .


###  https://auroral.iot.linkeddata.es/def/core#serialNumber
core:serialNumber rdf:type owl:DatatypeProperty ;
                  rdfs:domain saref:Device ;
                  rdfs:range xsd:string .


###  https://auroral.iot.linkeddata.es/def/core#versionNumber
core:versionNumber rdf:type owl:DatatypeProperty ;
                   rdfs:domain saref:Device ;
                   rdfs:range xsd:string .


