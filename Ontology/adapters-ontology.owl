#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/sosa/description
sosa:description rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/sosa/forProperty
sosa:forProperty rdf:type owl:ObjectProperty ;
                 rdfs:domain core:Actuator ;
                 rdfs:range <http://www.w3.org/ns/ssn/Property> ;
                 rdfs:comment "Presents the relationship between the actuator and the detected properties. It has a domain the Actuator and a range the ssn:Property" ;
                 rdfs:label "for property" .


###  http://www.w3.org/ns/sosa/observes
sosa:observes rdf:type owl:ObjectProperty ;
              rdfs:domain core:Sensor ;
              rdfs:range <http://www.w3.org/ns/ssn/Property> ;
              rdfs:comment "The relation between the Sensor device and the property. It has the Sensor as domain and the ssn:property the range." ;
              rdfs:label "observes" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#ID-Device
:ID-Device rdf:type owl:DatatypeProperty ;
           rdfs:domain :EnergyMonitor ,
                       :FloodSensor ,
                       :GPSEmergencyButton ,
                       :HumiditySensor ,
                       :MonitoringSensor ,
                       :MotionSensor ,
                       :SoilMoistureSEnsor ,
                       :Thermometer ,
                       :WasteBinSensor ,
                       :WaterAMRSensor ,
                       :WaterValveActuator ,
                       :WeatherSensor ,
                       core:Actuator ,
                       core:Device ,
                       core:Sensor ;
           rdfs:comment "Device Identifier" ;
           rdfs:label "ID Device" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#from-timestamp
:from-timestamp rdf:type owl:DatatypeProperty ;
                rdfs:domain :MonitoringSensor ,
                            :WeatherSensor ;
                rdfs:comment "UTC time in ISO 8601 formatted timestamp for when to get data From" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#has-location
:has-location rdf:type owl:DatatypeProperty ;
              rdfs:domain :EnergyMonitor ,
                          :FloodSensor ,
                          :GPSEmergencyButton ,
                          :HumiditySensor ,
                          :MonitoringSensor ,
                          :MotionSensor ,
                          :SoilMoistureSEnsor ,
                          :Thermometer ,
                          :WasteBinSensor ,
                          :WaterAMRSensor ,
                          :WaterValveActuator ,
                          :WeatherSensor ,
                          core:Actuator ,
                          core:Device ,
                          core:Sensor ;
              rdfs:comment "location of the device" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#has-timestamp
:has-timestamp rdf:type owl:DatatypeProperty ;
               rdfs:domain :MotionSensor ,
                           :SoilMoistureSEnsor ,
                           :WasteBinSensor ,
                           :WaterAMRSensor ,
                           :WeatherSensor ;
               rdfs:comment "Timestamp when the measure was detected" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#to-timestamp
:to-timestamp rdf:type owl:DatatypeProperty ;
              rdfs:comment "UTC time in ISO 8601 formatted timestamp for when to get data TO" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasHeight
:hasHeight rdf:type owl:DatatypeProperty ;
           rdfs:domain :SoilMoisture ;
           rdfs:range xsd:float ;
           rdfs:label "has height" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasVolume
:hasVolume rdf:type owl:DatatypeProperty ;
           rdfs:domain :SoilMoisture ;
           rdfs:range xsd:float ;
           rdfs:label "has volume" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#AmbientProperty
:AmbientProperty rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                 rdfs:comment "Ambient Property" ,
                              "This concepts regroups the ambient properties that are detcetd by the deployed devices. These properties are the ambient temperature, the ambient humidity, the ambient flood, the ambient wind and the ambient solar radiation properties." .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#AmbientSolarRadiation
:AmbientSolarRadiation rdf:type owl:Class ;
                       rdfs:subClassOf :AmbientProperty ;
                       rdfs:comment "Often called the solar resource or just sunlight, is a general term for the electromagnetic radiation emitted by the sun." ;
                       rdfs:label "Ambient Solar Radiation" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#CrabonDioxide
:CrabonDioxide rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
               rdfs:comment "regroups measurement about the CO2 concentration" ;
               rdfs:label "CO2 Carbon Dioxide" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#EnergyMonitor
:EnergyMonitor rdf:type owl:Class ;
               rdfs:subClassOf core:Sensor ;
               rdfs:comment "It is used to track and control real time energy use. It has different properties such as the returned energy, the total returned energy, etc..." ;
               rdfs:label "Energy Monitor Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#EnergyProperty
:EnergyProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:comment "It regroups the properties related to the energy such as: the battery level, the totalreturnedEnergy, the TotalEnergy, the PowerFactor, the Gridvoltage, and so one." ;
                rdfs:label "Energy Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#FloodAmbientProperty
:FloodAmbientProperty rdf:type owl:Class ;
                      rdfs:subClassOf :AmbientProperty ;
                      rdfs:comment "a rising and overflowing of a body os water especially onto normally dry land" ;
                      rdfs:label "Flood Ambient Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#FloodSensor
:FloodSensor rdf:type owl:Class ;
             rdfs:subClassOf core:Sensor ;
             rdfs:comment "It is designed for detection of water leaks or detecting excessively high-water level." ;
             rdfs:label "Flood Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#GPSEmergencyButton
:GPSEmergencyButton rdf:type owl:Class ;
                    rdfs:subClassOf core:Sensor ;
                    rdfs:comment "It is a device used to detect the exact location of a specific thing or object. The detected data is the longitude, latitude, altitude, and the acceleration." ;
                    rdfs:label "GPS Emergency Button" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#HumidityAmbientProperty
:HumidityAmbientProperty rdf:type owl:Class ;
                         rdfs:subClassOf :AmbientProperty ;
                         rdfs:comment "It is a value expressed as a percentage (0-100%). It is the amount of water vapor present in a specific volume." ;
                         rdfs:label "Ambient Humidity Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#HumiditySensor
:HumiditySensor rdf:type owl:Class ;
                rdfs:subClassOf core:Sensor ;
                rdfs:comment "It is a device that detects and measures water vapor in its environment and converts its findings into a corresponding electrical signal." ;
                rdfs:label "Humidity Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#LightProperty
:LightProperty rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
               rdfs:comment "it describes properties about illuminance." ;
               rdfs:label "Light Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#LuminusFlux
:LuminusFlux rdf:type owl:Class ;
             rdfs:subClassOf :LightProperty ;
             rdfs:label "Luminus Flux" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#MonitoringSensor
:MonitoringSensor rdf:type owl:Class ;
                  rdfs:subClassOf core:Sensor ;
                  rdfs:comment "It generates data about the active power, the reactive inductive power, the active enrgy, the reactive capacitif, the voltage and so one." ;
                  rdfs:label "Monitoring Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#MotionProperty
:MotionProperty rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
                rdfs:comment "it is  about the motion and the vibration properties." ;
                rdfs:label "Motion Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#MotionSensor
:MotionSensor rdf:type owl:Class ;
              rdfs:subClassOf core:Sensor ;
              rdfs:comment "A motion sensor (or motion detector) is a device that is designed to detect and measure movement." ;
              rdfs:label "Motion Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#PersonCounter
:PersonCounter rdf:type owl:Class ;
               rdfs:subClassOf :TRAFXProperty .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#PersonCounterSensor
:PersonCounterSensor rdf:type owl:Class ;
                     rdfs:subClassOf core:Sensor ;
                     rdfs:comment "The person sounter sensor is designed to count the number of person that visit a specific city" ;
                     rdfs:label "Peron Counter Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#Pressure
:Pressure rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
          rdfs:comment "provides information about the pressure properties (for example the atmospheric pressure)." ;
          rdfs:label "Pressure" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#QRCounter
:QRCounter rdf:type owl:Class ;
           rdfs:subClassOf :TRAFXProperty .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#QRGeneratorSensor
:QRGeneratorSensor rdf:type owl:Class ;
                   rdfs:subClassOf core:Sensor ;
                   rdfs:comment "The QR Generator sensor is designed to count the numebr of person that scan the QR code when they visit a specific city" ;
                   rdfs:label "QR Generator Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#SoilMoisture
:SoilMoisture rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
              rdfs:comment "presents the soil moisture height and volume" ;
              rdfs:label "Soil Moisture" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#SoilMoistureSEnsor
:SoilMoistureSEnsor rdf:type owl:Class ;
                    rdfs:subClassOf core:Sensor ;
                    rdfs:comment "soil moisture sensors measure the volumetric water content in soil. it provides data about the height and volume of water in soil." ;
                    rdfs:label "Soil Moisture Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#Sound
:Sound rdf:type owl:Class ;
       rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
       rdfs:comment "This concept defines the Noise Level properties." ;
       rdfs:label "Sound Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#SpaceProperty
:SpaceProperty rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> ;
               rdfs:comment "describes properties related to space such as the  GPS latitude, the GPS altitude, the GPS longitude, the XAxis Acceleration, the YAxisAcceleration, the ZAxis acceleration, the distance, etc." ;
               rdfs:label "Space Property" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#TRAFXProperty
:TRAFXProperty rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#TemperatureAmbientProperty
:TemperatureAmbientProperty rdf:type owl:Class ;
                            rdfs:subClassOf :AmbientProperty ;
                            rdfs:comment "Ambient Temperature is a measure of the temperature around a given asset or piece of equipement or other object" ;
                            rdfs:label "Ambient Temperature" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#Thermometer
:Thermometer rdf:type owl:Class ;
             rdfs:subClassOf core:Sensor ;
             rdfs:comment "A thermometer sensor is a device used to measure temperature." ;
             rdfs:label "Thermometer Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#WasteBinSensor
:WasteBinSensor rdf:type owl:Class ;
                rdfs:subClassOf core:Sensor ;
                rdfs:comment "It ensures a real time monitoring of the waste bins' status. It generates data about the distance and the deviation position" ;
                rdfs:label "Waste Bin Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#WaterAMRSensor
:WaterAMRSensor rdf:type owl:Class ;
                rdfs:subClassOf core:Sensor ;
                rdfs:comment "Water AMR sensor sends data about the accumulated value of water in the current timestamp." ;
                rdfs:label "Water AMR Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#WaterValveActuator
:WaterValveActuator rdf:type owl:Class ;
                    rdfs:subClassOf core:Actuator ;
                    rdfs:comment "it is used to control the water valve. It generates an acknowledgement message indicating that the command has been accepted. The generated command can be: ON, OFF, active, close." ;
                    rdfs:label "WaterValveActuator" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#WeatherSensor
:WeatherSensor rdf:type owl:Class ;
               rdfs:subClassOf core:Sensor ;
               rdfs:comment "It is a component of an automatic weather station that detects the presence of hydrometeors and determines their type (rain, snow, drizzle, etc.) and intensity. it detects data about the temperature, the humidity, the solar irradiation, the pressure and the rain." ;
               rdfs:label "Weather Sensor" .


###  http://www.semanticweb.org/user/ontologies/2022/5/adapters#WindAmbientProperty
:WindAmbientProperty rdf:type owl:Class ;
                     rdfs:subClassOf :AmbientProperty ;
                     rdfs:label "Wind Ambient Property" .


###  http://www.w3.org/ns/ssn/Property
<http://www.w3.org/ns/ssn/Property> rdf:type owl:Class ;
                                    rdfs:comment "It refers to the observables properties concerning a particular domain or use case. It has diverse sub-classes as presented below." ;
                                    rdfs:label "ssn:Property" .


###  https://auroral.iot.linkeddata.es/def/adapters#AccumulatedPercipitation
:AccumulatedPercipitation rdf:type owl:Class ;
                          rdfs:subClassOf <http://www.w3.org/ns/ssn/Property> .


###  https://auroral.iot.linkeddata.es/def/core#Actuator
core:Actuator rdf:type owl:Class ;
              rdfs:subClassOf core:Device ;
              rdfs:comment "A device that is used by, or implements, an (Actuation) Procedure that changes the state of the world" ;
              rdfs:label "Actuator" .


###  https://auroral.iot.linkeddata.es/def/core#Device
core:Device rdf:type owl:Class ;
            rdfs:comment "It is designed to realize a specific task. This task can be a detection of an occurred event or the initiation of an action. It has two sub-classes (sensor and actuator)." ;
            rdfs:label "Device" .


###  https://auroral.iot.linkeddata.es/def/core#Sensor
core:Sensor rdf:type owl:Class ;
            rdfs:subClassOf core:Device ;
            rdfs:comment "it responds to a Stimulus, e.g., a change in the environment, or Input data composed from the Results of prior Observations, and generate a Result" ;
            rdfs:label "Sensor" .


