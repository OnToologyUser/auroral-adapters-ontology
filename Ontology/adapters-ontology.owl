<https://auroral.iot.linkeddata.es/def/adapters> rdf:type owl:Ontology ;
                                                  <http://purl.org/dc/elements/1.1/creator> "Ahlem Rhayem" ,
                                                                                            "Andrea Cimmino" ,
                                                                                            "Juan Cano de Benito" ,
                                                                                            "Maria Poveda Villalon" ,
                                                                                            "Raúl García Castro" ;
                                                  <http://purl.org/dc/elements/1.1/description> "This ontology aims at modelling the adapters for the AURORAL project." ;
                                                  <http://purl.org/dc/elements/1.1/title> "The AURORAL adapters ontology" ;
                                                  <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
